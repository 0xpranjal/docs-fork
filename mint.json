{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Vapi",
  "logo": {
    "dark": "/static/images/logo/logo-dark.png",
    "light": "/static/images/logo/logo-light.png",
    "href": "/"
  },
  "favicon": "/static/images/favicon.png",
  "colors": {
    "primary": "#37AA9D",
    "light": "#5DFECA",
    "dark": "#37AA9D",
    "background": {
      "dark": "#121418"
    },
    "anchors": {
      "from": "#37AA9D",
      "to": "#5DFECA"
    }
  },
  "modeToggle": {
    "default": "dark",
    "isHidden": false
  },

  "topbarLinks": [
    {
      "name": "Home",
      "url": "https://vapi.ai"
    },
    {
      "name": "Pricing",
      "url": "/pricing"
    },
    {
      "name": "Status",
      "url": "https://status.vapi.ai"
    },
    {
      "name": "Support",
      "url": "mailto:support@vapi.ai"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://dashboard.vapi.ai"
  },

  "tabs": [
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "icon": "bolt",
      "iconType": "regular",
      "name": "Quickstart",
      "url": "quickstart"
    },
    {
      "icon": "gauge",
      "iconType": "regular",
      "name": "Dashboard",
      "url": "https://dashboard.vapi.ai"
    },
    {
      "icon": "discord",
      "iconType": "regular",
      "name": "Community",
      "url": "https://discord.gg/pUFNcf2WmH"
    },
    {
      "icon": "github",
      "iconType": "regular",
      "name": "GitHub",
      "url": "https://github.com/VapiAI"
    }
  ],
  "navigation": [
    {
      "group": "Welcome",
      "pages": [
        "introduction",
        "pricing",
        "support",
        "faq",
        "resources",
        "status"
      ]
    },
    {
      "group": "Get Started",
      "pages": [
        "quickstart",
        {
          "group": "Quickstart Guides",
          "pages": ["quickstart/dashboard"]
        }
      ]
    },
    {
      "group": "Concepts",
      "pages": [
        "assistants",
        "phone_calling",
        "clients",
        "server_url",
        {
          "group": "Advanced",
          "pages": [
            "provider-keys",
            "custom-llm",
            "custom_voice",
            "call_functions",
            "persistent_assistants"
          ]
        }
      ]
    },
    {
      "group": "Example Workflows",
      "pages": [
        "workflows/outbound-sales",
        "workflows/inbound-support",
        "pizza_website",
        "text_message",
        "outbound_call_python",
        "voice_widget"
      ]
    },
    {
      "group": "Guides",
      "pages": ["multilingual_support", "privacy_hipaa"]
    },
    {
      "group": "Platform",
      "pages": ["platform/how-vapi-works"]
    },
    {
      "group": "Knowledge",
      "pages": ["glossary", "faq"]
    },
    {
      "group": "Legal",
      "pages": ["privacy", "tos"]
    },
    {
      "group": "",
      "pages": ["api-reference/swagger", "api-reference/openapi"]
    },
    {
      "group": "Calls",
      "pages": [
        "api-reference/calls/list-calls",
        "api-reference/calls/get-call",
        "api-reference/calls/create-phone-call"
      ]
    },
    {
      "group": "Assistants",
      "pages": [
        "api-reference/assistants/list-assistants",
        "api-reference/assistants/create-assistant",
        "api-reference/assistants/get-assistant",
        "api-reference/assistants/delete-assistant",
        "api-reference/assistants/update-assistant"
      ]
    },
    {
      "group": "Phone Numbers",
      "pages": [
        "api-reference/phone-numbers/buy-phone-number",
        "api-reference/phone-numbers/import-twilio-number",
        "api-reference/phone-numbers/list-phone-numbers",
        "api-reference/phone-numbers/get-phone-number",
        "api-reference/phone-numbers/delete-phone-number",
        "api-reference/phone-numbers/update-phone-number"
      ]
    }
  ],

  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "footerSocials": {
    "github": "https://github.com/vapiai",
    "twitter": "https://twitter.com/vapi_ai",
    "linkedin": "https://www.linkedin.com/company/vapi-ai",
    "website": "https://vapi.ai"
  },

  "openapi": "api.json",

  "search": {
    "prompt": "Search or ask..."
  },
  "seo": {
    "indexHiddenPages": true
  },
  "redirects": [
    {
      "source": "/getting_started",
      "destination": "/quickstart/dashboard"
    },
    {
      "source": "/dashboard",
      "destination": "/quickstart/dashboard"
    },

    {
      "source": "/provider_keys",
      "destination": "/provider-keys"
    },
    {
      "source": "/custom_llm",
      "destination": "/custom-llm"
    },
    {
      "source": "/outbound_sales",
      "destination": "/workflows/outbound-sales"
    },
    {
      "source": "/technical_support",
      "destination": "/workflows/inbound-support"
    }
  ]
}
